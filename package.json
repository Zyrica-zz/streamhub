{
  "name": "streamhub",
  "version": "0.0.1",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.0.0",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "scripts": {
    "build": "rimraf /dist | npm run build:server | npm run build:client",
    "build:server": "babel server --out-dir dist",
    "build:client": "cross-env NODE_ENV=production webpack",
    "dev:client": "webpack-dev-server --content-base public",
    "dev:server": "nodemon --exec babel-node server",
    "start": "node dist"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  }
